{
  "name": "q_ide_default",
  "description": "Default agent configuration",
  "prompt": "",
  "mcpServers": {
    "clowee_erp": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-postgres"
      ],
      "env": {
        "PGHOST": "localhost",
        "PGPORT": "5433",
        "PGUSER": "postgres",
        "PGPASSWORD": "clowee@erp",
        "PGDATABASE": "clowee_erp",
        "PGSSL": "false"
      },
      "timeout": 60000,
      "disabled": false
    }
  },
  "tools": [
    "fs_read",
    "execute_bash",
    "fs_write",
    "report_issue",
    "use_aws",
    "@clowee_erp"
  ],
  "toolAliases": {},
  "allowedTools": [
    "fs_read",
    "report_issue",
    "use_aws",
    "execute_bash",
    "fs_write",
    "@clowee_erp/connect_db",
    "@clowee_erp/query",
    "@clowee_erp/list_schemas",
    "@clowee_erp/list_tables",
    "@clowee_erp/describe_table"
  ],
  "toolsSettings": {
    "use_aws": {
      "preset": "readOnly"
    },
    "execute_bash": {
      "preset": "readOnly"
    }
  },
  "resources": [
    "file://AmazonQ.md",
    "file://README.md",
    "file://.amazonq/rules/**/*.md"
  ],
  "hooks": {
    "agentSpawn": [],
    "userPromptSubmit": []
  },
  "useLegacyMcpJson": true
}